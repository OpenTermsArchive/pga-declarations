{
  "name": "Spotify",
  "importedFrom": "https://github.com/tosdr/tosback2/blob/f762cd4bbb2571272985fed009201a23300ba9b2/rules/spotify.com.xml",
  "terms": {
    "Privacy Policy": {
      "fetch": "https://www.spotify.com/us/legal/privacy-policy/",
      "select": [
        {
          "startBefore": "h1",
          "endBefore": "footer"
        }
      ],
      "remove": "[href='#spotify-privacy-policy']"
    },
    "Terms of Service": {
      "fetch": "https://www.spotify.com/us/legal/end-user-agreement/",
      "select": [
        {
          "startBefore": "#spotify-terms-of-use",
          "endBefore": "footer"
        }
      ]
    },
    "Acceptable Use Policy": {
      "fetch": "https://www.spotify.com/us/legal/user-guidelines/",
      "select": [
        {
          "startBefore": "#spotify-user-guidelines",
          "endBefore": "footer"
        }
      ]
    },
    "Community Guidelines": {
      "select": [
        {
          "startBefore": "#start-of-content",
          "endBefore": "footer"
        }
      ],
      "remove": [
        "[data-encore-id=type]",
        "button",
        "[data-encore-id=buttonSecondary]",
        "[data-encore-id=textLink]",
        "img"
      ],
      "combine": [
        {
          "fetch": "https://www.spotify.com/de-en/safetyandprivacy/platform-rules"
        },
        {
          "fetch": "https://www.spotify.com/de-en/safetyandprivacy/content-actions"
        },
        {
          "fetch": "https://www.spotify.com/de-en/safetyandprivacy/reporting-content"
        },
        {
          "fetch": "https://www.spotify.com/de-en/safetyandprivacy/parental-guide"
        },
        {
          "fetch": "https://www.spotify.com/de-en/safetyandprivacy/age-assurance"
        },
        {
          "fetch": "https://www.spotify.com/de-en/safetyandprivacy/elections-integrity"
        },
        {
          "fetch": "https://www.spotify.com/de-en/safetyandprivacy/dangerous-deceptive-content"
        },
        {
          "fetch": "https://www.spotify.com/de-en/safetyandprivacy/violent-extremism"
        },
        {
          "fetch": "https://www.spotify.com/de-en/safetyandprivacy/understanding-recommendations"
        },
        {
          "fetch": "https://www.spotify.com/de-en/safetyandprivacy/dsa-points-of-contact"
        },
        {
          "fetch": "https://www.spotify.com/de-en/safetyandprivacy/personal-data-collected"
        },
        {
          "fetch": "https://www.spotify.com/de-en/safetyandprivacy/protecting-personal-data"
        },
        {
          "fetch": "https://www.spotify.com/de-en/safetyandprivacy/privacy-controls"
        },
        {
          "fetch": "https://www.spotify.com/de-en/safetyandprivacy/transparency"
        }
      ]
    },
    "Trackers Policy": {
      "fetch": "https://www.spotify.com/us/legal/cookies-policy/?_ga=2.256460875.1965153890.1707895974-688248205.1707895974",
      "select": [
        "main"
      ],
      "remove": [
        "[class*=subnav-main]"
      ]
    },
    "Content Monetisation Policy": {
      "fetch": "https://support.spotify.com/us/creators/article/spotify-partner-program-monetization-policies/",
      "select": [
        "main"
      ],
      "remove": [
        "[class*=subnav-main]"
      ]
    },
    "executeClientScripts": true
  }
}