{
  "name": "TikTok",
  "documents": {
    "Terms of Service": {
      "fetch": "https://www.tiktok.com/legal/terms-of-service-eea?lang=en&selection=true",
      "select": "article",
      "remove": [
        ".language-selection",
        "img"
      ],
      "executeClientScripts": true
    },
    "Privacy Policy": {
      "fetch": "https://www.tiktok.com/legal/page/eea/privacy-policy/en",
      "select": "div[data-testid=\"policy-card\"]",
      "remove": [
        ".language-selection",
        "img"
      ]
    },
    "Community Guidelines": {
      "fetch": "https://www.tiktok.com/community-guidelines?lang=en",
      "select": ".base-layout-container",
      "remove": ".syl-image-wrapper"
    },
    "Law Enforcement Guidelines": {
      "fetch": "https://www.tiktok.com/legal/law-enforcement?lang=en",
      "select": "article"
    },
    "Copyright Claims Policy": {
      "fetch": "https://www.tiktok.com/legal/copyright-policy?lang=en",
      "select": "article"
    },
    "In-App Purchases Policy": {
      "fetch": "https://www.tiktok.com/legal/virtual-items-eea?lang=en&selection=true",
      "select": "article",
      "remove": [
        ".language-selection",
        "img"
      ],
      "executeClientScripts": true
    },
    "Trackers Policy": {
      "fetch": "https://www.tiktok.com/legal/tiktok-website-cookies-policy?lang=en",
      "select": "article"
    }
  }
}
