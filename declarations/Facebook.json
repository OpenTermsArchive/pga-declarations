      "fetch": "https://www.facebook.com/legal/terms/plain_text_terms",
      "select": [
        {
          "startBefore": "div[role=main]",
          "endBefore": "[role=\"separator\"]:first-child"
        }
      ],
      "filter": [
        "cleanUrls",
        "replaceInfoImageWithEmoji"
      ],
      "executeClientScripts": true
    },
