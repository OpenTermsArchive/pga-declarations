{
  "name": "Facebook",
  "documents": {
    "Terms of Service": {
      "fetch": "https://www.facebook.com/legal/terms/plain_text_terms",
      "select": [
        {
          "startBefore": "div[role=main]",
          "endBefore": "[role=\"separator\"]:first-child"
        }
      ],
      "filter": [
        "cleanUrls",
        "replaceInfoImageWithEmoji"
      ],
      "executeClientScripts": true
    },
    "Privacy Policy": {
      "fetch": "https://mbasic.facebook.com/privacy/policy/printable/",
      "select": [
        "#root"
      ],
      "remove": [
        ".k",
        ".l",
        ".bc",
        "img[style=\"display:none\"]"
      ],
      "filter": [
        "removeTrackingIDs"
      ]
    },
    "Developer Terms": {
      "fetch": "https://developers.facebook.com/policy",
      "filter": [
        "removeHelpButtons",
        "cleanUrls"
      ],
      "select": "._4bl9"
    },
    "Trackers Policy": {
      "fetch": "https://www.facebook.com/policies/cookies",
      "filter": [
        "removeImgFromCookiesPolicy",
        "removeReturnToTopButtons",
        "cleanUrls"
      ],
      "select": "._5tkp"
    },
    "Commercial Terms": {
      "fetch": "https://www.facebook.com/legal/commercial_terms",
      "select": [
        {
          "startBefore": "div[role=main]",
          "endBefore": "[role=\"separator\"]:first-child"
        }
      ],
      "filter": [
        "cleanUrls"
      ],
      "executeClientScripts": true
    },
    "Data Processor Agreement": {
      "fetch": "https://www.facebook.com/legal/EU_data_transfer_addendum",
      "select": [
        "div[role=main]"
      ],
      "remove": [
        ".rq0escxv .l9j0dhe7 .du4w35lb .j83agx80 .cbu4d94t .pfnyh3mw .d2edcug0 .pybr56ya"
      ],
      "filter": [
        "removeTrackingIDs"
      ],
      "executeClientScripts": true
    },
    "Live Policy": {
      "fetch": "https://en-gb.facebook.com/policies/live",
      "select": [
        {
          "startBefore": "div[role=main]",
          "endBefore": "[role=\"separator\"]:first-child"
        }
      ],
      "filter": [
        "removeTrackingIDs"
      ],
      "executeClientScripts": true
    },
    "Law Enforcement Guidelines": {
      "fetch": "https://www.facebook.com/safety/groups/law/guidelines",
      "filter": [
        "cleanUrls"
      ],
      "select": [
        "._4xy4 > div"
      ],
      "remove": [
        ".img"
      ]
    }
  }
}
